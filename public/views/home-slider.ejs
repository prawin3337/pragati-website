<section id="aa-slider">
    <div class="aa-slider-area">
      <div id="sequence" class="seq">
        <div class="seq-screen">
          <ul class="seq-canvas">
            <!-- single slide item -->
            <li>
              <div class="seq-model">
                <img data-seq src="<%= domain %>/img/slider/1.jpg" alt="Men slide img" />
              </div>
              <div class="seq-title">
                <h2 data-seq><span>Solar Products</span></h2>
                <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn">know more</a>
              </div>
            </li>
            <!-- single slide item -->
            <li>
              <div class="seq-model">
                <img data-seq src="<%= domain %>/img/slider/2.jpg" alt="Wristwatch slide img" />
              </div>
              <div class="seq-title">
                <h2 data-seq><span>LED lighting solutions</span></h2>
                
                <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn">know more</a>
              </div>
            </li>
            <!-- single slide item -->
            <li>
              <div class="seq-model">
                <img data-seq src="<%= domain %>/img/slider/3.jpg" alt="Women Jeans slide img" />
              </div>
              <div class="seq-title">
                <span  data-seq class="hidden">Save Up to 75% Off</span>
                <h2 data-seq><span>aviation lamp</span></h2>
                
                <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn">know more</a>
              </div>
            </li>
             <li>
              <div class="seq-model">
                <img data-seq src="<%= domain %>/img/slider/4.jpg" alt="wireless communication device" />
              </div>
              <div class="seq-title">
                <h2 data-seq><span>wireless communication device</span></h2>
                
                <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn">know more</a>
              </div>
            </li>
          </ul>
        </div>
        <!-- slider navigation btn -->
        <fieldset class="seq-nav" aria-controls="sequence" aria-label="Slider buttons">
          <a type="button" class="seq-prev" aria-label="Previous"><span class="fa fa-angle-left"></span></a>
          <a type="button" class="seq-next" aria-label="Next"><span class="fa fa-angle-right"></span></a>
        </fieldset>
      </div>
    </div>
  </section>