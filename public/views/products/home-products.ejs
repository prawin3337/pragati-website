<section id="aa-product">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="row">
            <div class="aa-product-area">
              <div class="aa-product-inner">
                <!-- start prduct navigation -->
                  <ul class="nav nav-tabs aa-products-tab">
                    <li class="active">
                      <a href="#all" data-toggle="tab">All</a>
                    </li>
                    <% categorys.forEach(({categoryId, categoryName, disabled}, i) => {
                      if(disabled != "true") { %>
                        <li>
                          <a href="#<%= categoryId %>" data-toggle="tab"><%= categoryName %></a>
                        </li>
                    <% }}); %>
                  </ul>
                  <!-- Tab panes -->
                  <div class="tab-content">
                    <!-- Start men product category -->
                    <div class="tab-pane fade in active" id="all">
                      <ul class="aa-product-catg">
                        <% products.forEach((hPro) => { %>
                          <!-- start single product item -->
                          <li>
                            <figure>
                              <a class="aa-product-img" href="<%= domain %>/product/details?productId=<%= hPro.productId %>">
                                <% if(hPro.productImages) {%><img src="<%= domain %>/img/<%= hPro.productImages[0] %>" alt=""></a> <%}%>
                              <a class="aa-add-card-btn" href="<%= domain %>/product/details?productId=<%= hPro.productId %>"><i class="fa fa-info-circle" aria-hidden="true"></i> &nbsp;</span>view details</a>
                                <figcaption>
                                <h4 class="aa-product-title"><a href="<%= domain %>/product/details?productId=<%= hPro.productId %>"><%= hPro.productName %></a></h4>
                                <span class="aa-product-price hidden"><%= hPro.productPrice %></span><span class="aa-product-price hidden"><del>$65.50</del></span>
                              </figcaption>
                            </figure>
                            <div class="aa-product-hvr-content">
                              <a href="#" data-toggle2="tooltip" data-placement="top" title="Quick View" data-val="<%= JSON.stringify(hPro)%>" data-toggle="modal" data-target="#quick-view-modal"><span class="fa fa-search"></span></a>
                            </div>
                            <!-- product badge -->
                            <!-- <span class="aa-badge aa-sale" href="#">SALE!</span> -->
                          </li>
                        <% }) %>
                      </ul>
                      <div class="aa-browse-container">
                        <a class="aa-browse-btn" href="<%=domain%>/product/all-products">Browse all Product
                          <span class="fa fa-long-arrow-right"></span>
                        </a></div>
                    </div>

                    <% categorys.forEach(({categoryId, categoryName}, i) => { %>
                      <div class="tab-pane fade" id="<%= categoryId %>">
                        <ul class="aa-product-catg">
                          <% const hProducts = products.filter((pro) => pro.categoryId == categoryId)%>
                          <% hProducts.forEach((hPro) => { %>
                            <!-- start single product item -->
                            <li>
                              <figure>
                                <a class="aa-product-img" href="<%= domain %>/product/details?productId=<%= hPro.productId %>">
                                  <% if(hPro.productImages) {%><img src="<%= domain %>/img/<%= hPro.productImages[0] %>" alt=""></a><%}%>
                                <a class="aa-add-card-btn" href="<%= domain %>/product/details?productId=<%= hPro.productId %>"><i class="fa fa-info-circle" aria-hidden="true"></i> &nbsp;</span>view details</a>
                                  <figcaption>
                                  <h4 class="aa-product-title"><a href="<%= domain %>/product/details?productId=<%= hPro.productId %>"><%= hPro.productName %></a></h4>
                                  <span class="aa-product-price hidden"><%= hPro.productPrice %></span><span class="aa-product-price hidden"><del>$65.50</del></span>
                                </figcaption>
                              </figure>
                              <div class="aa-product-hvr-content">
                                <a href="#" data-toggle2="tooltip" data-placement="top" title="Quick View" data-val="<%= JSON.stringify(hPro)%>" data-toggle="modal" data-target="#quick-view-modal"><span class="fa fa-search"></span></a>
                              </div>
                              <!-- product badge -->
                              <!-- <span class="aa-badge aa-sale" href="#">SALE!</span> -->
                            </li>
                          <% }) %>
                        </ul>
                        <div class="aa-browse-container">
                          <a class="aa-browse-btn" href="<%=domain%>/product/all-products?categoryId=<%=categoryId%>&&categoryName=<%=categoryName%>">
                            Browse all Product <span class="fa fa-long-arrow-right"></span>
                          </a>
                        </div>
                      </div>
                    <% }); %>
                    <!-- / sports product category -->
                  </div>
                  <!-- quick view modal -->
                  <%- include('product-quick-view'); -%>
                  <!-- / quick view modal -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>